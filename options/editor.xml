<application>
  <component name="CodeInsightSettings">
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="inheritors" />
        <option value="problems" />
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
    <option name="enabled" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DeclarativeInlayHintsSettings">
    <option name="providerIdToEnabled">
      <map>
        <entry key="groovy.implicit.null.argument" value="false" />
        <entry key="groovy.lambda.parameter.inlay.provider" value="false" />
        <entry key="java.implicit.lambdaParameter" value="false" />
        <entry key="java.method.chains" value="false" />
      </map>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JAVA.chain.hints" />
        <option value="JAVA.java.implicit.types" />
        <option value="R.return.values.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="R.return.values.hints" />
    <settingsMapElement>
      <R.return.values.hints>
        <Settings>
          <option name="showImplicitReturn" value="false" />
        </Settings>
      </R.return.values.hints>
    </settingsMapElement>
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseOneLineMethods" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\bTODO\b.*" />
    <pattern case-sensitive="true" pattern="\bFAZER\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="\bTODOX\b.*" />
    <filter name="Urgente">
      <pattern index="2" />
    </filter>
    <filter name="Comum">
      <pattern index="0" />
      <pattern index="1" />
    </filter>
  </component>
</application>